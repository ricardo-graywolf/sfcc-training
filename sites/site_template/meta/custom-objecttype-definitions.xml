<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31"/>
    <type-extension type-id="SitePreferences">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="GTMId">
                <display-name xml:lang="x-default">Google tag manager id</display-name>
                <description xml:lang="x-default">GTM Id to add code generated in Google Tag Manager.</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value></default-value>
            </attribute-definition>
             <attribute-definition attribute-id="GTMEnable">
                <display-name xml:lang="x-default">Google tag manager enable</display-name>
                <description xml:lang="x-default">Enable or disables GTM script</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value></default-value>
            </attribute-definition>
		</custom-attribute-definitions>
        <group-definitions>
            <attribute-group group-id="GTM">
                <display-name xml:lang="x-default">Google Tag Manager</display-name>
                <attribute attribute-id="GTMId"/>
                <attribute attribute-id="GTMEnable"/>
            </attribute-group>
        </group-definitions>
    </type-extension>
</metadata>